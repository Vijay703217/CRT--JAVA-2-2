 import java.util.*;

class  Elaseif

{
	public static void main(String[]args)
	{
		Scanner s=new Scanner(System.in);
		

	int a,b,c;
     System.out.println("enter a values");

	 a=s.nextInt();

	 b=s.nextInt();
	  
	 c=s.nextInt();
	 if  (a<b && a<c)
		
		{
		 System.out.println("smallest  a no="+a);
 		}
     else
		 if(b<c)
		{
		 System.out.println("smallest  b  no="+b);

		}
		else
		{
			System.out.println("smallest  c no="+c);
	}
	}}
